{
    "logging": {
        "dir": "/root/cufile_logs",
        "level": "INFO"
    },
    "profile": {
        "nvtx": false,
        "cufile_stats": 0
    },
    "execution": {
        "max_io_queue_depth": 128,
        "max_io_threads": 4,
        "parallel_io": true,
        "min_io_threshold_size_kb": 8192,
        "max_request_parallelism": 4
    },
    "properties": {
        "max_direct_io_size_kb": 16384,
        "max_device_cache_size_kb": 131072,
        "per_buffer_cache_size_kb": 1024,
        "max_device_pinned_mem_size_kb": 33554432,
        "use_poll_mode": false,
        "poll_mode_max_size_kb": 4,
        "use_pci_p2pdma": false,
        "allow_compat_mode": false,
        "gds_rdma_write_support": true,
        "io_batchsize": 128,
        "io_priority": "default",
        "rdma_load_balancing_policy": "RoundRobin",
        "rdma_dynamic_routing": false,
        "rdma_dynamic_routing_order": [
            "GPU_MEM_NVLINKS",
            "GPU_MEM",
            "SYS_MEM",
            "P2P"
        ],
        "rdma_access_mask": "0x1f",
        "rdma_dev_addr_list": [
            "{{ mlx5_5_ip.stdout }}",
            "{{ mlx5_4_ip.stdout }}",
            "{{ mlx5_3_ip.stdout }}",
            "{{ mlx5_2_ip.stdout }}"
        ]
    },
    "fs": {
        "generic": {
            "posix_unaligned_writes": false
        },
        "beegfs": {
            "posix_gds_min_kb": 0
        },
        "lustre": {
            "posix_gds_min_kb": 0
        },
        "nfs": {},
        "gpfs": {
            "gds_write_support": true,
            "rdma_dev_addr_list": [
                "{{ mlx5_5_ip.stdout }}",
                "{{ mlx5_4_ip.stdout }}",
                "{{ mlx5_3_ip.stdout }}",
                "{{ mlx5_2_ip.stdout }}"
            ]
        },
        "weka": {
            "rdma_write_support": false
        }
    },
    "denylist": {
        "drivers": [],
        "devices": [],
        "mounts": [],
        "filesystems": []
    },
    "miscellaneous": {
        "api_check_aggressive": false
    }
}
